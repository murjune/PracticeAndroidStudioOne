<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.seminar2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".util.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Seminar2">
        <activity
            android:name=".presentation.LottoActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".coroutine.PracticeActivity"
            android:exported="true"></activity>
        <activity
            android:name=".coroutine.CoroutinActivity"
            android:exported="true"></activity>
        <activity
            android:name=".CalculatorActivity"
            android:exported="true"
            android:label="@string/title_activity_view_model_sample" />
        <activity
            android:name=".SearchActivity"
            android:exported="true"
            android:label="@string/title_activity_search"
            android:theme="@style/Theme.Seminar2.NoActionBar"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>